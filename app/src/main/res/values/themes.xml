    <style name="Theme.examoneOone" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/theme</item>
        <item name="colorPrimaryDark">@color/theme</item>
        <item name="colorPrimaryVariant">@color/status_bar</item>
        <item name="colorOnPrimary">@color/secondaryColor</item>
